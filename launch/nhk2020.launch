<launch>
  <!--for navigation-->
  <include file="$(find nhk_2020)/launch/run.launch"/>
  <node pkg="rosserial_python" type="serial_node.py" name="imu">
    <param name="port" value="/dev/ttyACM1"/>
  </node>
  <node pkg="imu_filter_madgwick" type="imu_filter_node" name="madgwick_filter_node">
    <param name="fixed_frame" value="odom"/>
    <param name="world_frame" value="enu"/>
    <param name="use_magnetic_field_msg" value="true"/>
  </node>
  <node pkg="rosserial_python" type="serial_node.py" name="PS3_arduino">
    <param name="port" value="/dev/ttyACM0"/>
  </node>
  <node pkg="rosserial_python" type="serial_node.py" name="main">
    <param name="port" value="/dev/ttyACM2"/>
  </node>
  <node pkg="nhk_2020" type="ps3" name="ps3">
  </node>

</launch>
